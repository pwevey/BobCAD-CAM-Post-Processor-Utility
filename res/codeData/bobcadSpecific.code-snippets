		"Add Encryption (BobCAD)": {
			"scope": "bcpst",
			"prefix": "encryption",
			"body": [
			  "<--BEGIN ENCRYPT",
			  "",
			  "// place all desired post scripting blocks here",
			  "",
			  "<--END ENCRYPT"
			],
			"description": "Post Block Format: Add encryption to a post block to protect your post processor code. Once added, open BobCAD, go to CAM tab > Post Encryption to encrypt the post processor. This will prevent others from viewing or editing your post processor code."
		},
		"Begin Encryption (BobCAD)": {
			"scope": "bcpst",
			"prefix": "begin encrypt",
			"body": [
			  "<--BEGIN ENCRYPT",
			],
			"description": "Post Block Format: Add encryption to a post block to protect your post processor code. Use <--END ENCRYPT to close off the encryption. Once added, open BobCAD, go to CAM tab > Post Encryption to encrypt the post processor. This will prevent others from viewing or editing your post processor code."
		},
		"End Encryption (BobCAD)": {
			"scope": "bcpst",
			"prefix": "end encrypt",
			"body": [
			  "<--END ENCRYPT",
			],
			"description": "Post Block Format: Add encryption to a post block to protect your post processor code. Use <--BEGIN ENCRYPT to start the encryption. Once added, open BobCAD, go to CAM tab > Post Encryption to encrypt the post processor. This will prevent others from viewing or editing your post processor code."
		},
